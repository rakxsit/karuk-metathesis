			</s>
			<s>
				<kar>
					<w>
						<m id="4715">páy</m>
					</w>
					<w>
						<m id="6307">uum</m>
					</w>
					<w gl="because of">
						<m id="4408">pa</m>
						<m id="3971">kumá'ii</m>
					</w>
					<w gl="his stomach">
						<m id="4407">pa</m>
						<m id="6581">vishváan</m>
