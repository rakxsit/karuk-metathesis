						<m id="4296">na</m>
						<m id="6906">xuniháyaachha</m>
					</w>.
				</kar>
				<eng>I'm really hungry!</eng>
			</s>
			<s>
				<kar>
					<w>
						<m id="4224">matêe</m>
					</w>
					<w gl="let me count">
						<m id="3735">kan</m>
						<m id="2674">ip</m>
						<m id="6123">tôor</m>
						<m id="1419">i</m>
					</w>
