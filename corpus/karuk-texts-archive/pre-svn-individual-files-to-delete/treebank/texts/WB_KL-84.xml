    <data>
        <paragraph>
			<s>
                <txt lang="kyh">
                    <w>
						<m lemma="3753">káruk</m>
					</w>
					<w gl="there was a world renewal">
						<m lemma="6175">u</m>
						<m lemma="2882">'ír</m>
						<m lemma="202">ahiv</m>
					</w>.
				</txt>
				<trans lang="eng">There was a world renewal upriver.</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w gl="it is called">
						<m lemma="6175">ú</m>
						<m lemma="3323">thvuuy</m>
						<m lemma="6035">ti</m>
					</w>
					<w>
						<m lemma="3717">kah'ínaam</m>
					</w>.
				</txt>
				<trans lang="eng">(The place) is called kah'ínaam (Clear Creek).</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="5485">surukúnvuunup</m>
					</w>
					<w>
						<m lemma="6383">vaa</m>
					</w>
					<w>
						<m lemma="3670">káan</m>
					</w>
					<w>
						<m lemma="10">aah</m>
					</w>
					<w gl="they were making it">
						<m lemma="3977">kun</m>
						<m lemma="2348">ikyâa</m>
						<m lemma="6035">tih</m>	
					</w>
					<w gl="they were shooting at targets">
						<m lemma="3977">kun</m>
						<m lemma="3102">íshriim</m>
						<m lemma="6035">tih</m>
					</w>.
				</txt>
				<trans lang="eng">They made a fire there at surukúnvuunup (Sing-Ho Bar), they shot at targets.</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w gl="first of all">
						<m lemma="4976">pishiich</m>
						<m lemma="5522">tâapas</m>
					</w>
					<w gl="they were shooting at targets">
						<m lemma="3977">kun</m>
						<m lemma="3102">íshriim</m>
						<m lemma="6035">tih</m>
					</w>.
				</txt>
				<trans lang="eng">They shot at targets first of all.</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="2392">imáankam</m>
					</w>
					<w gl="on a height downriver from xumvároov">
						<m lemma="6904">xumvaroov</m>
						<m lemma="7213">yúrukam</m>
					</w>
					<w>
						<m lemma="3978">kúna</m>
					</w>
					<w gl="they would shoot at targets">
						<m lemma="3977">kun</m>
						<m lemma="3102">ishrív</m>
						<m lemma="1379">eesh</m>
					</w>.
				</txt>
				<trans lang="eng">The next day they shot at targets on a height downriver from xumvároov.</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="2392">imáankam</m>
					</w>
					<w>
						<m lemma="5715">tasáxaak</m>
					</w>
					<w gl="behind it">
						<m lemma="3940">kú</m>
						<m lemma="4146">maam</m>
					</w>
					<w gl="they were shooting at targets">
						<m lemma="3977">kun</m>
						<m lemma="3102">íshriim</m>
						<m lemma="6035">tih</m>
					</w>.
				</txt>
				<trans lang="eng">The next day they shot at targets behind tasáxaak.</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="7145">yuhsahním'anamahach</m>
					</w>
					<w>
						<m lemma="7097">yítha</m>
					</w>
					<w>
						<m lemma="5506">tá</m>
					</w>
					<w gl="they spent the night">
						<m lemma="3977">kun</m>
						<m lemma="2186">ikvêesh</m>
					</w>.
				</txt>
				<trans lang="eng">They spent one night at yuhsahním'anamahach.</trans>
			</s>
		</paragraph>
