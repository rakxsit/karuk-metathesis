					<w>
						<m lemma="6847">xás</m>
					</w>
					<w>
						<m lemma="6383">vaa</m>
					</w>
					<w>
						<m lemma="7085">yíiv</m>
					</w>
					<w>
						<m lemma="7212">yúruk</m>
					</w>
					<w gl="uphill from xumvároov">
						<m lemma="6904">xumvaroo</m>
						<m lemma="4213">máruk</m>
					</w>
