   <text id="WB_KL-55">
		<metadata>
			<title>"Wrestling Medicine"</title>
			<author>Mamie <alph>Offield</alph></author>
			<date>1957</date>
			<publication>William Bright, <em>The Karok Language</em> (1957), pp. 260-261, Text 55</publication>
		</metadata>
		<data>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3207">itáharavan</m>
						</w>
						<w gl="they grew up">
							<m lemma="3977">kun</m>
							<m lemma="1640">'íifshipree</m>
							<m lemma="1369">nik</m>
						</w>
						<w gl="brothers">
							<m lemma="6081">tipahêer</m>
							<m lemma="584">as</m>
						</w>.
					</txt>
					<trans lang="eng">Ten brothers once grew up.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="edge of the fireplace">
							<m lemma="197">ahinám</m>
							<m lemma="6047">tiich</m>
						</w>
						<w>
							<m lemma="6675">vúra</m>
						</w>
						<w gl="he sat">
							<m lemma="6175">u</m>
							<m lemma="1974">krêe</m>
							<m lemma="1369">nik</m>
						</w>
						<w>
							<m lemma="353">amtápnihich</m>
						</w>.
					</txt>
					<trans lang="eng">And (one of them, named kunâach'aa) sat at the edge of the fireplace, in the ashes.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="they said">
							<m lemma="3977">kun</m>
							<m lemma="4823">piip</m>
						</w>, "
						<w>
							<m lemma="4215">maruk'áraar</m>
						</w>
						<w>
							<m lemma="6383">vaa</m>
						</w>
						<w>
							<m lemma="3670">káan</m>
						</w>
						<w gl="edge of the lake">
							<m lemma="6222">úknam</m>
							<m lemma="6047">tiimich</m>
						</w>
						<w gl="he is staying">
							<m lemma="6175">ú</m>
							<m lemma="1974">krii</m>
						</w>.
					</txt>
					<trans lang="eng">And they said, "A giant is staying there at the edge of the lake.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6570">víri</m>
						</w>
						<w>
							<m lemma="234">akâay</m>
						</w>
						<w>
							<m lemma="6675">vúra</m>
						</w>
						<w>
							<m lemma="6757">xákaan</m>
						</w>
						<w gl="they will wrestle">
							<m lemma="3977">kun</m>
							<m lemma="6710">vúunv</m>
							<m lemma="1379">eesh</m>
						</w>."
					</txt>
					<trans lang="eng">Who will wrestle with him?"</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6174">ta'ítam</m>
						</w>
						<w gl="he thought">
							<m lemma="6175">u</m>
							<m lemma="6939">xús</m>
							<m lemma="1369">anik</m>
						</w>
						<w gl="the strongest one">
							<m lemma="4407">p</m>
							<m lemma="1905">eekpihan</m>
							<m lemma="5522">tâapas</m>
						</w>, "
						<w>
							<m lemma="6042">tîi</m>
						</w>
						<w>
							<m lemma="6757">xákaan</m>
						</w>
						<w gl="let us wrestle!">
							<m lemma="4370">nú</m>
							<m lemma="6710">vuunv</m>
							<m lemma="1419">i</m>
						</w>."
					</txt>
					<trans lang="eng">So the strongest one thought, "Let me wrestle with him!"</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="when he went">
							<m lemma="4408">p</m>
							<m lemma="6175">oo</m>
							<m lemma="6396">vâaram</m>
						</w>,
						<w gl="towards uphill a little">
							<m lemma="4147">mâam</m>
							<m lemma="6444">vanih</m>
							<m lemma="1420">ich</m>
						</w>
						<w gl="he had">
							<m lemma="5506">t</m>
							<m lemma="6175">óo</m>
						</w>
						<w>
							<m lemma="1786">kfuukra</m>
						</w>.
					</txt>
					<trans lang="eng">So when he went, he climbed a little ways uphill.</trans>
				</s>
			</paragraph>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he rushed outdoors">
							<m lemma="6175">u</m>
							<m lemma="569">'árihrupuk</m>
						</w>,
						<w>
							<m lemma="3980">kunâach'aa</m>
						</w>.
					</txt>
					<trans lang="eng">Then kunâach'aa went outdoors.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="a palmful">
							<m lemma="809">átruup</m>
							<m lemma="1011">'axyar</m>
						</w>
						<w>
							<m lemma="346">ámtaap</m>
						</w>
						<w gl="he carried it outdoors in his hand">
							<m lemma="6175">u</m>
							<m lemma="4283">mûut</m>
							<m lemma="5199">rupuk</m>
						</w>.
					</txt>
					<trans lang="eng">And he brought a handful of ashes outdoors.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he shouted">
							<m lemma="6175">u</m>
							<m lemma="1902">kpêehva</m>
						</w>.
					</txt>
					<trans lang="eng">And he shouted.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w>
							<m lemma="6383">vaa</m>
						</w>
						<w gl="he thought">
							<m lemma="6175">u</m>
							<m lemma="4823">piip</m>
						</w>, "
						<w>
							<m lemma="1660">iimkun</m>
						</w>
						<w>
							<m lemma="1402">úm</m>
						</w>
						<w>
							<m lemma="4059">kúth</m>
						</w>
						<w gl="I am growing up">
							<m lemma="4349">ni</m>
							<m lemma="1453">'íif</m>
							<m lemma="6035">tih</m>
						</w>,
						<w>
							<m lemma="1660">iimkun</m>
						</w>
						<w>
							<m lemma="1402">úm</m>
						</w>
						<w>
							<m lemma="3971">kumá'ii</m>
						</w>
						<w gl="I am growing up">	
							<m lemma="4349">ni</m>
							<m lemma="1453">'íif</m>
							<m lemma="6035">tih</m>
						</w>.
					</txt>
					<trans lang="eng">And he said, "Am I growing up for you people, am I growing up for your sake?</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="4299">naa</m>
						</w>
						<w>
							<m lemma="3752">káru</m>
						</w>
						<w>
							<m lemma="6675">vúra</m>
						</w>
						<w>
							<m lemma="3670">káan</m>
						</w>
						<w gl="I am growing up">
							<m lemma="4349">ni</m>
							<m lemma="1453">'íif</m>
							<m lemma="6035">tih</m>
						</w>."
					</txt>
					<trans lang="eng">I am growing up here too!"</trans>
				</s>
			</paragraph>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he heard">
							<m lemma="6175">u</m>
							<m lemma="5954">thítiv</m>
						</w>,
						<w gl="visibly it">
							<m lemma="7037">y</m>
							<m lemma="6175">óo</m>
						</w>
						<w>
							<m lemma="1438">chrívchav</m>
						</w>
						<w gl="the water">
							<m lemma="4407">pá</m>
							<m lemma="50">'aas</m>
						</w>,
						<w gl="in the lake">
							<m lemma="4407">pa</m>
							<m lemma="6222">'úkraam</m>
						</w>,
						<w gl="when they threw him into the water">
							<m lemma="4408">pa</m>
							<m lemma="3977">kun</m>
							<m lemma="4429">páathkuri</m>
						</w>
						<w gl="his brother">
							<m lemma="4407">pa</m>
							<m lemma="4260">mu</m>
							<m lemma="6081">típah</m>
						</w>.
					</txt>
					<trans lang="eng">Then he heard it, he saw the water splash in the lake, when (the giant) threw his brother in.</trans>
					<note>Use of kun- in this sentence and the following I think is for literary effect, to make it seem like there are many antagonists or to obscure him. I have glossed the subject as "they" to try and preserve this.</note>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="1096">chavúra</m>
						</w>
						<w>
							<m lemma="3930">koovúra</m>
						</w>
						<w>
							<m lemma="5506">tá</m>
						</w>
						<w gl="they threw them into the water">
							<m lemma="3977">kun</m>
							<m lemma="3551">ixyákurih</m>
						</w>.
					</txt>
					<trans lang="eng">Finally (the giant) threw all (the brothers) in.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3930">koovúra</m>
						</w>
						<w>
							<m lemma="6383">vaa</m>
						</w>
						<w gl="he was saying">
							<m lemma="6175">u</m>
							<m lemma="4823">pí</m>
							<m lemma="6035">tih</m>
						</w>, "
						<w>
							<m lemma="1660">iimkun</m>
						</w>
						<w>
							<m lemma="1402">úm</m>
						</w>
						<w>
							<m lemma="4059">kúth</m>
						</w>
						<w gl="I am growing up">
							<m lemma="4349">ni</m>
							<m lemma="1453">'íif</m>
							<m lemma="6035">tih</m>
						</w>.
					</txt>
					<trans lang="eng">Every time, (kunâach'aa) said, "Am I growing up for you people?</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="4299">naa</m>
						</w>
						<w>
							<m lemma="3752">káru</m>
						</w>
						<w>
							<m lemma="6675">vúra</m>
						</w>
						<w>
							<m lemma="3670">káan</m>
						</w>
						<w gl="I am growing up">
							<m lemma="4349">ni</m>
							<m lemma="1453">'íif</m>
							<m lemma="6035">tih</m>
						</w>,
						<w>
							<m lemma="3980">kunâach'aa</m>
						</w>."
					</txt>
					<trans lang="eng">I am growing up here too, (I,) kunâach'aa!"</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he thought">
							<m lemma="6175">u</m>
							<m lemma="6939">xus</m>
						</w>, "
						<w>
							<m lemma="1410">hûut</m>
						</w>
						<w gl="I will do it">
							<m lemma="4349">ni</m>
							<m lemma="4086">kuuph</m>
							<m lemma="1379">eesh</m>
						</w>.
					</txt>
					<trans lang="eng">And he thought, "What shall I do?</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="4450">pácheech</m>
						</w>
						<w>
							<m lemma="5506">tá</m>
						</w>
						<w gl="I remain">
							<m lemma="4349">ni</m>
							<m lemma="5206">saam</m>
						</w>.
					</txt>
					<trans lang="eng">I am left alone.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="1144">chími</m>
						</w>
						<w gl="let me go!">
							<m lemma="3735">kan</m>
							<m lemma="6396">vâaram</m>
							<m lemma="1419">i</m>
						</w>
						<w gl="I myself">
							<m lemma="4299">ná</m>
							<m lemma="4259">mpaan</m>
						</w>.
					</txt>
					<trans lang="eng">Let <em>me</em> go (to wrestle)!"</trans>
					<note>Bright underlines "me" in the English translation</note>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6174">ta'ítam</m>
						</w>
						<w gl="he went">
							<m lemma="6175">u</m>
							<m lemma="6396">vâaram</m>
							<m lemma="1378">aheen</m>
						</w>.
					</txt>
					<trans lang="eng">So he left.</trans>
					<note>Bright does not offer an English translation for this sentence. Sentence 19 of his English translation corresponds to sentence 20 of the Karuk. The English of sentence 19 seems to not fit either Karuk 19 or 20 very well, but seems to be a mix of the two.</note>
				</s>
			</paragraph>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w>
							<m lemma="6383">vaa</m>
						</w>
						<w>
							<m lemma="3670">káan</m>
						</w>
						<w gl="he arrived">
							<m lemma="6175">u</m>
							<m lemma="6306">'uum</m>
						</w>.
					</txt>
					<trans lang="eng">So he went there.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6174">ta'ítam</m>
						</w>
						<w gl="he laughed">
							<m lemma="6175">u</m>
							<m lemma="2079">ksáh</m>
							<m lemma="1378">een</m>
						</w>
						<w gl="the giant">
							<m lemma="4407">pa</m>
							<m lemma="4215">maruk'áraar</m>
						</w>,
						<w gl="he thought">
							<m lemma="6175">u</m>
							<m lemma="6939">xus</m>
						</w>, "
						<w>
							<m lemma="1457">íf</m>
						</w>
						<w>
							<m lemma="4352">nîinamich</m>
						</w>
						<w gl="for together">
							<m lemma="4408">pa</m>
							<m lemma="6756">xákaan</m>
						</w>
						<w gl="we are going to wrestle">
							<m lemma="4370">nu</m>
							<m lemma="6710">vúunv</m>
							<m lemma="1379">eesh</m>
						</w>."
					</txt>
					<trans lang="eng">And the giant laughed, he thought, "He's really small for us to wrestle together!"</trans>
				</s>
			</paragraph>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w>
							<m lemma="6174">ta'ítam</m>
						</w>
						<w gl="they wrestled">
							<m lemma="3977">kun</m>
							<m lemma="6710">vúunv</m>
							<m lemma="1378">aheen</m>
						</w>.
					</txt>
					<trans lang="eng">So then they wrestled.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w>
							<m lemma="3930">koovúra</m>
						</w>
						<w gl="the little plants">
							<m lemma="4407">pa</m>
							<m lemma="4963">pinish</m>
							<m lemma="6128">tunvêech</m>
							<m lemma="584">as</m>
						</w>
						<w>
							<m lemma="3752">káru</m>
						</w>
						<w>
							<m lemma="3930">koovúra</m>
						</w>
						<w gl="the trees">
							<m lemma="4407">pa</m>
							<m lemma="2682">'ípaha</m>
						</w>
						<w gl="they were shouting to him">
							<m lemma="3977">kun</m>
							<m lemma="1632">ihyûunish</m>
							<m lemma="6035">tih</m>
						</w>, "
						<w>
							<m lemma="3980">kunâach'aa</m>
						</w>, 
						<w gl="go hard!">
							<m lemma="5156">puxîich</m>
							<m lemma="1419">i</m>
						</w>."
					</txt>
					<trans lang="eng">And all the little plants and all the trees shouted to him, "Go to it, kunâach'aa!"</trans>
				</s>
			</paragraph>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he threw him down">
							<m lemma="6175">u</m>
							<m lemma="4452">páchish</m>
						</w>
						<w gl="the giant">
							<m lemma="4407">pa</m>
							<m lemma="4215">maruk'áraar</m>
						</w>,
						<w>
							<m lemma="6222">ukráam</m>
						</w>
						<w gl="he threw him into water">
							<m lemma="6175">u</m>
							<m lemma="4429">paathkúrih</m>
						</w>.
					</txt>
					<trans lang="eng">Then he threw the giant down, he threw him in the lake.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6174">ta'ítam</m>
						</w>
						<w>
							<m lemma="3930">koovúra</m>
						</w>
						<w gl="they came back to life">
							<m lemma="3977">kun</m>
							<m lemma="4925">pimtáv</m>
							<m lemma="1378">aheen</m>
						</w>
						<w gl="his brothers">
							<m lemma="4407">pa</m>
							<m lemma="4260">mu</m>
							<m lemma="6081">tipáh</m>
							<m lemma="1724">iivshas</m>
						</w>.
					</txt>
					<trans lang="eng">And all his brothers came back to life.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6174">ta'ítam</m>
						</w>
						<w gl="they went back home">
							<m lemma="3977">kun</m>
							<m lemma="4695">pávyiihshipr</m>
							<m lemma="1378">een</m>
						</w>.
					</txt>
					<trans lang="eng">So they went back home.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="5163">púyava</m>
						</w>
						<w>
							<m lemma="6383">vaa</m>
						</w>
						<w gl="he did it">
							<m lemma="6175">u</m>
							<m lemma="4086">kúphaa</m>
							<m lemma="1369">nik</m>
						</w>
						<w>
							<m lemma="3980">kunâach'aa</m>
						</w>.
					</txt>
					<trans lang="eng">kunâach'aa did that.</trans>
				</s>
			</paragraph>
