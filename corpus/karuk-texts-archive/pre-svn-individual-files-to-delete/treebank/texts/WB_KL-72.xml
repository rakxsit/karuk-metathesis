						<m lemma="4407" id="WB_KL-72:51" head="WB_KL-72:50" synrel="ATR">pa</m>
						<m lemma="4264">mukun</m>
						<m lemma="6952">xúskaamhar</m>
					</w>
                    <w gl="so" id="WB_KL-72:52" head="WB_KL-72:56" synrel="DIS">
						<m lemma="6570">víri</m>
						<m lemma="6383" id="WB_KL-72:53" head="WB_KL-72:56" synrel="DIS">va</m>
					</w>
