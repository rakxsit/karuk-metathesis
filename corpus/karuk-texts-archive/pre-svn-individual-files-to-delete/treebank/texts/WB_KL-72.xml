					</w>
                    <w gl="when it had become day" id="WB_KL-72:26" head="WB_KL-72:34" synrel="ADV">
						<m lemma="4408" id="WB_KL-72:27" head="WB_KL-72:26" synrel="SUB">pa</m>
						<m lemma="5506" id="WB_KL-72:28" head="WB_KL-72:26" synrel="TAM">t</m>
						<m lemma="6175">u</m>
						<m lemma="5469">súpaah</m>
						<m lemma="1358">aak</m>
