					</w>
                    <w id="WB_KL-72:32" head="WB_KL-72:34" synrel="COMP">
						<m lemma="4076">kúuk</m>
					</w>
                    <w id="WB_KL-72:33" head="WB_KL-72:34" synrel="TAM">
						<m lemma="5506">tá</m>
					</w>
                    <w gl="they ran to there" id="WB_KL-72:34" head="0" synrel="PRED">
						<m lemma="3977">kun</m>
						<m lemma="1562">ihmárava</m>
					</w>, 
					<w id="WB_KL-72:35" head="WB_KL-72:36" synrel="TAM">
						<m lemma="5506">tá</m>
					</w>
