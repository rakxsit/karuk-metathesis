   <text id="WB_KL-04">
		<metadata>
			<title>"Coyote's Journey"</title>
			<author>Julia <alph>Starritt</alph></author>
			<date>1957</date>
			<publication>William Bright, <em>The Karok Language</em> (1957), pp. 176-181, Text 4</publication>
		</metadata>
		<data>
			<paragraph>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="4782">pihnêefich</m>
						</w>
						<w>
							<m lemma="6307">uum</m>
						</w>
						<w gl="money-making">
							<m lemma="3054">ishpuk</m>
							<m lemma="2357">éekyav</m>
						</w>
						<w gl="he had">	
							<m lemma="5506">t</m>
							<m lemma="6175">óo</m>
						</w>
						<w>
							<m lemma="3317">thvoonha</m>
						</w>.
					</txt>
					<trans lang="eng">Coyote wanted to make money.</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he said">
							<m lemma="6175">u</m>
							<m lemma="4823">píip</m>
						</w> "
						<w>
							<m lemma="3725">kahyúras</m>
						</w>
						<w>
							<m lemma="5506">tá</m>
						</w>
						<w gl="I leave">
							<m lemma="4349">ni</m>
							<m lemma="6396">vâaram</m>
						</w>, 
						<w>
							<m lemma="3054">ishpúk</m>
						</w>
						<w>
							<m lemma="5506">tá</m>
						</w>
						<w gl="I go in order to get it">
							<m lemma="4349">ni</m>
							<m lemma="2334">kyáar</m>
						</w>."
					</txt>
					<trans lang="eng">And he said, "I'm leaving for Klamath Lakes, I'm going to make money."</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w gl="he went upriver">
							<m lemma="6175">u</m>
							<m lemma="567">'árihroov</m>
						</w>, 
						<w gl="he was singing songs">
							<m lemma="6175">u</m>
							<m lemma="4511">pakurîihvu</m>
							<m lemma="6035">tih</m>
						</w>.
					</txt>
					<trans lang="eng">And he went upriver, he was singing.</trans>
				</s>
				<s>
					<txt lang="kyh">
						"
						<w>
							<m lemma="3725">kahyúras</m>
						</w>
						<w gl="I will leave">
							<m lemma="4349">ni</m>
							<m lemma="6396">vâaram</m>
							<m lemma="1379">eesh</m>
						</w>, 
						<w>
							<m lemma="3054">ishpúk</m>
						</w>
						<w gl="I will go to get it">
							<m lemma="4349">ni</m>
							<m lemma="2334">kyâar</m>
							<m lemma="1379">eesh</m>
						</w>."
					</txt>
					<trans lang="eng">"I'll leave for Klamath Lakes, I'll go make money."</trans>
				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w>
							<m lemma="6675">vúra</m>
						</w>
						<w>
							<m lemma="7085">yíiv</m>
						</w>
						<w>
							<m lemma="3753">káruk</m>
						</w>
						<w gl="he had gone upriver">
							<m lemma="5506">t</m>
							<m lemma="6175">u</m>
							<m lemma="567">'árihroov</m>
						</w>.
					</txt>
					<trans lang="eng">And he went a long ways upriver.</trans>
				</s>
			</paragraph>
