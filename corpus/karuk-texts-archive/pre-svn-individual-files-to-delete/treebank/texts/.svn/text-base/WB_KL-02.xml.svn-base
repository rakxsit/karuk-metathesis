				<txt lang="kyh">
					<w>
						<m lemma="3745">kári</m>
					</w>
					<w>
						<m lemma="6847">xás</m>
					</w>
					<w>
						<m lemma="5506">tá</m>
					</w>
					<w gl="they said">
						<m lemma="3977">kun</m>
						<m lemma="4823">piip</m>
					</w>, "
					<w>
						<m lemma="1144">chími</m>
					</w>
					<w>
						<m lemma="561">árihishrih</m>
					</w>."
				</txt>
				<trans lang="eng">And they said, "Sing!"</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="5163">púyava</m>
					</w>
					<w>
						<m lemma="3930">koovúra</m>
					</w>
					<w>
						<m lemma="5506">tá</m>
					</w>
					<w gl="they sang songs">
						<m lemma="3977">kun</m>
						<m lemma="4511">pakúriihvu</m>
						<m lemma="4298">naa</m>
					</w>.
				</txt>
				<trans lang="eng">So they all sang.</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="5163">púyava</m>
					</w> "
					<w>
						<m lemma="7058">yee</m>
					</w>!
					<w>
						<m lemma="1144">chími</m>
					</w>
					<w>
						<m lemma="2474">ímpaan</m>
					</w>
					<w>
						<m lemma="247">akich</m>
					</w>."
				</txt>
				<trans lang="eng">Then (they said), "Well, how about you (singing), friend!"</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="5541">táay</m>
					</w>
					<w>
						<m lemma="5506">tá</m>
					</w>
