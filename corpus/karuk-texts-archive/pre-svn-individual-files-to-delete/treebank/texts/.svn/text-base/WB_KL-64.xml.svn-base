				<txt lang="kyh">
					<w>
						<m lemma="3745">kári</m>
					</w>
					<w>
						<m lemma="6847">xás</m>
					</w>
					<w>
						<m lemma="2240">ikxáram</m>
					</w>
					<w>
						<m lemma="924">axmáy</m>
					</w>
					<w gl="she heard">
						<m lemma="6175">u</m>
						<m lemma="5954">thítiv</m>
					</w>,
					<w>
						<m lemma="924">axmáy</m>
					</w>
					<w gl="he entered the house">
						<m lemma="6175">u</m>
						<m lemma="6606">vôonfuruk</m>
