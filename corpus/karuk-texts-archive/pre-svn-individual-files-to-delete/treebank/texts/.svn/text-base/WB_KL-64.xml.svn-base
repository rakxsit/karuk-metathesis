			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="4224">matêe</m>
					</w>
					<w gl="let me count">
						<m lemma="3735">kan</m>
						<m lemma="2674">ip</m>
						<m lemma="6123">tôor</m>
