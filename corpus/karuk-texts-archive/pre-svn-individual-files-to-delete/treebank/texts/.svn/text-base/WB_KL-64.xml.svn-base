			<s>
				<txt lang="kyh">
					<w>
						<m lemma="3745">kári</m>
					</w>
					<w>
						<m lemma="6847">xás</m>
					</w>
					<w gl="he said">
						<m lemma="6175">u</m>
						<m lemma="4823">piip</m>
					</w>,
					"<w gl ="little niece">
						<m lemma="2949">ishávaas</m>
						<m lemma="1420">ich</m>
					</w>
					<w>
						<m lemma="1396">hôoy</m>
					</w>
					<w>
						<m lemma="739">áta</m>
					</w>
					<w gl="she went">
						<m lemma="6175">u</m>
						<m lemma="6396">vâaram</m>
						<m lemma="1378">aheen</m>
					</w>.
				</txt>
				<trans lang="eng">And he said, "I wonder where little niece has gone?</trans>
			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="1457">íf</m>
					</w>
					<w>
						<m lemma="5506">ta</m>
					</w>
					<w gl="I am hungry">
						<m lemma="4296">na</m>
						<m lemma="6906">xuniháyaachha</m>
					</w>.
				</txt>
