							<m lemma="828">ávan</m>
						</w>
						<w gl="she will have">
							<m lemma="6175">u</m>
							<m lemma="5916">thiiná</m>
							<m lemma="6035">tih</m>
							<m lemma="1379">eesh</m>
						</w>,
						<w>
							<m lemma="6734">xáat</m>
						</w>
						<w gl="a homely woman">
							<m lemma="656">asiktavan</m>
							<m lemma="3790">kéem</m>
