					<txt lang="kyh">
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
						<w>
							<m lemma="6383">vaa</m>
						</w>
						<w gl="they did">
