				</s>
				<s>
					<txt lang="kyh">
						<w>
							<m lemma="6570">víri</m>
						</w>
						<w>
							<m lemma="3745">kári</m>
						</w>
						<w>
							<m lemma="6847">xás</m>
						</w>
