					<txt lang="kyh">
						<w gl="so">
							<m lemma="6570">víri</m>
							<m lemma="6383">va</m>
						</w>
						<w>
							<m lemma="6675">vúra</m>
						</w>
						<w>
							<m lemma="828">ávan</m>
						</w>
