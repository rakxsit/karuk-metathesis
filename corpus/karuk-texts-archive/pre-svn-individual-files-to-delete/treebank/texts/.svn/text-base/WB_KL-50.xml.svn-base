							<m lemma="6997">yâahi</m>
							<m lemma="6035">tih</m>
							<m lemma="1371">ara</m>
						</w>,
						<w gl="where they were digging roots">
							<m lemma="4408">pa</m>
							<m lemma="3977">kun</m>
							<m lemma="6321">'ûupvu</m>
							<m lemma="4298">naa</m>
							<m lemma="6035">tih</m>
