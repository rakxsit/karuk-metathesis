						<m lemma="5082" id="JPH_KT-01b:33" head="JPH_KT-01b:32" synrel="NEG">pu</m>
						<m lemma="3845">kin</m>
						<m lemma="3580">îik<sup>y</sup>ár</m>
						<m lemma="1379">eesh</m>
						<m lemma="1371">ara</m>
					</w>,<!-- accent should probably be the second one according to WB (although somewhat unclear), but JPH writes both-->
					<w id="JPH_KT-01b:34" head="JPH_KT-01b:38" synrel="DIS">
                        <m lemma="6383">vaa</m>
                    </w>
                    <w id="JPH_KT-01b:35" head="JPH_KT-01b:38" synrel="DIS">
                        <m lemma="6675">vúra</m>
                    </w>
                    <w id="JPH_KT-01b:36" head="JPH_KT-01b:37" synrel="ATR">
                        <m lemma="4715">páy</m>
                    </w>
                    <w id="JPH_KT-01b:37" head="JPH_KT-01b:38" synrel="COMP">
                        <m lemma="3922">k<sup>y</sup>ôomahich</m>
                    </w>
