			<s>
				<txt lang="kyh">
					<w>
						<m lemma="6847">xás</m>
					</w>
					<w gl="they said">
						<m lemma="3977">kun</m>
						<m lemma="4823">piip</m>
					</w>, "
					<w>
						<m lemma="7058">yée</m>
					</w>
					<w>
						<m lemma="4299">naa</m>
					</w>, 
					<w>
						<m lemma="1457">íf</m>
					</w>
					<w>
						<m lemma="4715">páy</m>
					</w>
					<w gl="the acorn soup">
						<m lemma="4407">pa</m>
						<m lemma="6965">xúun</m>
					</w>
					<w>
						<m lemma="332">amáyav</m>
					</w>."
				</txt>
				<trans lang="eng">And they said, "Say, this is really delicious acorn soup!"</trans>
			</s>
