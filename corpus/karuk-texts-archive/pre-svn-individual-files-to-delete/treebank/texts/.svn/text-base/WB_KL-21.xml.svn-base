				<txt lang="kyh">
					<w>
						<m lemma="6847">xás</m>
					</w>
					<w>
						<m lemma="3745">kári</m>
					</w>
					<w>
						<m lemma="3969">kúkuum</m>
					</w>
					<w gl="the young women">
						<m lemma="4407">pa</m>
						<m lemma="1459">'ifápiit</m>
						<m lemma="5200">sha</m>
					</w>
					<w>
						<m lemma="5506">tá</m>
					</w>
