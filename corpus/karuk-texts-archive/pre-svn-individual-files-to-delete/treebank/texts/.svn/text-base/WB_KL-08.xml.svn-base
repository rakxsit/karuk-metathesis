                        <m lemma="6035">ti</m>
                    </w>
                    <w>
                        <m lemma="6570">víri</m>
                    </w>
                    <w>
                        <m lemma="6383">vaa</m>
                    </w>
                    <w gl="he was picking up">
                        <m lemma="6175">u</m>
                        <m lemma="1468">'ífik</m>
                        <m lemma="6035">tih</m>
                    </w>.
                </txt>
                <trans lang="eng">
