            <s>
                <txt lang="kyh">
                    <w>
                        <m lemma="6847">xás</m>
                    </w>
                    <w>
                        <m lemma="4782">pihnêefich</m>
                    </w>
                    <w gl="he was happy">
                        <m lemma="6175">u</m>
                        <m lemma="2">'aachíchha</m>
                    </w>,
                    <w>
                        <m lemma="6847">xás</m>
                    </w>
                    <w gl="he said">
                        <m lemma="6175">u</m>
                        <m lemma="4823">píip</m>
                    </w>
