                    <w><m lemma="6675">vúra</m></w>
                    <w gl="the human"><m lemma="4407">pa</m><m lemma="506">'áraar</m></w>
                    <w><m lemma="6309">uumkun</m></w>
                    <w gl="they will do"><m lemma="3977">kun</m><m lemma="4014">kupheesh</m></w>, 
					<w gl="as I"><m lemma="4408">pá</m><m lemma="4299">naa</m></w>
                    <w><m lemma="5506">tá</m></w>
