			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="6847">xás</m>
					</w>
					<w gl="for too long">
						<m lemma="6839">xára</m>
						<m lemma="1389">hirurav</m>
					</w>
					<w gl="he was absent">
						<m lemma="6175">u</m>
						<m lemma="5414">sínmoo</m>
