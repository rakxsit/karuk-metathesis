			</s>
			<s>
				<txt lang="kyh">
					<w>
						<m lemma="6570">víri</m>
					</w>
					<w>
						<m lemma="6308">ûum</m>
					</w>
					<w>
						<m lemma="5506">tá</m>
					</w>
